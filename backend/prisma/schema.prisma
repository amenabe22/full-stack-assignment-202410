// prisma/schema.prisma
datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Conversation {
  id        String   @id @default(cuid())
  messages  String
  convId  String?
  isAI Boolean    @default(false)
  createdAt DateTime @default(now())
}
