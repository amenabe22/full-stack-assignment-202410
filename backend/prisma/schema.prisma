// prisma/schema.prisma
datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
  binaryTargets   = ["native", "linux-arm64-openssl-3.0.x"]
}

model Conversation {
  id        String   @id @default(cuid())
  messages  String
  convId  String?
  memId String?
  isAI Boolean    @default(false)
  createdAt DateTime @default(now())
}
